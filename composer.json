{
  "name": "madmages/xsd2php",
  "version": "1.1.0",
  "description": "Convert XSD (XML Schema) definitions into PHP classes and JMS metadata",
  "type": "library",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email": "goetas@gmail.com"
    },
    {
      "name": "Aleksandr Kalin",
      "email": "maddmages@gmail.com"
    }
  ],
  "keywords": [
    "converter",
    "xml",
    "xsd",
    "php",
    "jms",
    "serializer"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "symfony/yaml": "^2.1|^3|^4",
    "goetas-webservices/xsd-reader": "^0.2|^0.3",
    "doctrine/inflector": "^1",
    "zendframework/zend-code": "^3.0.3",
    "illuminate/container": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "jms/serializer": "1.9",
    "goetas-webservices/xsd2php-runtime": "0.2.7"
  },
  "autoload": {
    "psr-4": {
      "Madmages\\Xsd\\XsdToPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Madmages\\Xsd\\XsdToPhp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.0-dev"
    }
  }
}
